public class Main{
    public static void main(String args[]){
        String str="nandhu ka";
        System.out.println(str);
        for(int i=0;i<str.length();i++){
            for(int j=i+2;j<str.length();j+=2){
                System.out.println(str.substring(i,j));
            }
        }
    }
}
